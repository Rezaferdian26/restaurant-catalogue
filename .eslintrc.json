{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-undef":"off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle":"off",
        "no-plusplus":"off",
        "no-await-in-loop":"off",
        "no-console":"off",
        "no-alert":"off"
    }
}
